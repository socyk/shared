local library = loadstring(request({Url = "https://raw.githubusercontent.com/socyk/library/refs/heads/main/kylib",Method = "GET"}).Body)()

local tab_one = library.addtab("tab1")

tab_one.addtoggle({
	text = "example toggle (with keypicker)",
	default = false,
	callback = function(value)
		print(value)
	end
}).addkeypicker({
	default = nil,
})

tab_one.addslider({
	text = "example slider",
	min = 0,
	max = 100,
	default = 0,
	callback = function(value)
		print(value)
	end
})

tab_one.adddropdown({
	text = "example dropdown",
	values = {"none","one"},
	default = "none",
	callback = function(value)
		print(value)
	end
})

local tab_two = library.addtab("tab2")

tab_two.addtoggle({
	text = "example toggle",
	default = false,
	callback = function(value)
		print(value)
	end
})

tab_two.addtoggle({
	text = "example toggle",
	default = false,
	callback = function(value)
		print(value)
	end
})

tab_two.addslider({
	text = "example slider",
	min = 0,
	max = 100,
	default = 0,
	callback = function(value)
		print(value)
	end
})

tab_two.adddropdown({
	text = "example dropdown",
	values = {"none","one"},
	default = "none",
	callback = function(value)
		print(value)
	end
})

local tab_three = library.addtab("tab3")

tab_three.addslider({
	text = "example slider",
	min = 0,
	max = 100,
	default = 0,
	callback = function(value)
		print(value)
	end
})

tab_three.adddropdown({
	text = "example dropdown",
	values = {"none","one"},
	default = "none",
	callback = function(value)
		print(value)
	end
})

local menu = library.addtab("menu")

menu.addtoggle({
	text = "snow",
	default = true,
	callback = function(value)
		library.snow = value
	end
})
